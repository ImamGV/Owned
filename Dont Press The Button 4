local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Bacon-Library/refs/heads/main/bacon.txt"))()

local Tabs = {
Farm = Library:CreateWindow("Farm")
}
function Tween(Pos)
    Distance = (Pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    if game.Players.LocalPlayer.Character.Humanoid.Sit == true then game.Players.LocalPlayer.Character.Humanoid.Sit = false end
    pcall(function() tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(Distance/40, Enum.EasingStyle.Linear),{CFrame = Pos}) end)
    tween:Play()
    if Distance <= 10 then
        tween:Cancel()
    end
end

Library:CreateToggle(Tabs.Farm, "Auto Farm Pumpkins", false, function(v)
_G.AutoFarmPumpkins = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoFarmPumpkins then
for _,v in ipairs(workspace:GetChildren()) do
if v:IsA("Model") and v:FindFirstChild("Pumpkins"):FindFirstChild("Pumpkin") then
Tween(v:WaitForChild("Pumpkins"):WaitForChild("Pumpkin"):WaitForChild("Hitbox"))
end
end
end
end)
end)
